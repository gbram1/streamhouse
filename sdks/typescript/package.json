{
  "name": "streamhouse",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript client for StreamHouse streaming data platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "streamhouse",
    "streaming",
    "kafka",
    "data",
    "client",
    "typescript"
  ],
  "author": "StreamHouse Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/streamhouse/streamhouse.git",
    "directory": "sdks/typescript"
  },
  "homepage": "https://docs.streamhouse.io",
  "bugs": {
    "url": "https://github.com/streamhouse/streamhouse/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
